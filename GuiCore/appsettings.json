{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "HnD": {
    "VirtualRoot": "/",
    "DefaultFromEmailAddress": "notused@yourdomain.com",
    "DefaultToEmailAddress": "notused@yourdomain.com",
    "IPBanComplainEmailAddress": "boohoohoo@yourdomain.com",
    "SiteName": "LLBLGen Pro Support System",
    "EmailPasswordSubject": "Your password to access the forums on: ",
    "EmailThreadNotificationSubject": "A new message has been posted in a thread you're subscribed to on: ",
    "EmojiFilesPath": "/pics/emojis",
    "MaxAmountMessagesPerPage": "25",
    "DataFilesPath": "/DataFiles",
    "SmileyMappings": {
      ":)": "simple_smile",
      ":(": "rage",
      ":?": "confused",
      "8)": "sunglasses",
      ":/": "disappointed",
      ":!": "flushed",
      ":D": "smile",
      ";(": "cry",
      ":o": "frowning",
      ":P": "stuck_out_tongue_winking_eye",
      ";)": "wink"
    },
    "ResultsetCacheConfiguration":{
      "CacheMemoryLimitMegabytes": 64,
      "PhysicalMemoryLimitPercentage": 0,
      "PollingInterval": "00:00:30"
    }
  },
  "LLBLGenPro": {
    "CatalogNameOverwrites": {
      "HnD": "HnD_LLBLGen"
    },
    "SqlServerDQECompatibilityLevel": "6"
  },
  "AllowedHosts": "*"
